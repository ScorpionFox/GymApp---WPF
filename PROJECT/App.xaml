<Application x:Class="PROJECT.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:PROJECT"
             xmlns:ModelWidoku="clr-namespace:PROJECT.MVVM.ModelWidoku"
              xmlns:Widok="clr-namespace:PROJECT.MVVM.Widok"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Motywy/PrzyciskiMenu.xaml"/>
                <ResourceDictionary Source="Motywy/TextBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>

            <DataTemplate DataType="{x:Type ModelWidoku:HomeViewModel}">
                <Widok:HomeView/>
            </DataTemplate>
            
        </ResourceDictionary>
    </Application.Resources>
</Application>
